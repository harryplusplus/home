    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.6.12",
    "remark-toc": "9.0.0",
    "tailwindcss": "4.1.10"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-astro",
      "prettier-plugin-tailwindcss"
